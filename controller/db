CREATE TABLE `fclientes` (
	`FC_ID` INT(10) NOT NULL AUTO_INCREMENT,
	`FC_CODIGO` VARCHAR(80) NULL DEFAULT NULL,
	`FC_DESCRIPCION` VARCHAR(180) NULL DEFAULT NULL,
	`FC_TIPOCLIENTE` INT(3) NULL DEFAULT NULL,
	`FC_TIPODOC` INT(3) NULL DEFAULT NULL,
	`FC_DOCUMENTO` VARCHAR(50) NULL DEFAULT NULL,
	`FC_DIRECCION1` VARCHAR(200) NULL DEFAULT NULL,
	`FC_DIRECCION2` VARCHAR(200) NULL DEFAULT NULL,
	`FC_DIRECCION3` VARCHAR(200) NULL DEFAULT NULL,
	`FC_TELEFONO1` VARCHAR(50) NULL DEFAULT NULL,
	`FC_TELEFONO2` VARCHAR(50) NULL DEFAULT NULL,
	`FC_MOVIL1` VARCHAR(50) NULL DEFAULT NULL,
	`FC_MOVIL2` VARCHAR(50) NULL DEFAULT NULL,
	`FC_PERSONACONTACTO` VARCHAR(50) NULL DEFAULT NULL,
	`FC_EMAIL` VARCHAR(50) NULL DEFAULT NULL,
	`FC_EMAILALT` VARCHAR(50) NULL DEFAULT NULL,
	`FC_FECHAINICIO` DATE NULL DEFAULT NULL,
	`FC_FACEBOOK` VARCHAR(100) NULL DEFAULT NULL,
	`FC_TWITTER` VARCHAR(100) NULL DEFAULT NULL,
	`FC_INSTAGRAM` VARCHAR(100) NULL DEFAULT NULL,
	`FC_DIASCREDITO` INT(5) NULL DEFAULT '0',
	`FC_LIMITECREDITO` DECIMAL(10,2) NULL DEFAULT '0.00',
	`FC_TOLERANCIA` INT(5) NULL DEFAULT '0',
	`FC_TIPOPRECIO` INT(5) NULL DEFAULT '1',
	`FC_EXENTO` INT(5) NULL DEFAULT '0',
	`FC_STATUS` ENUM('activo','inactivo') NULL DEFAULT 'activo',
	`FC_FECHACREACION` DATE NULL DEFAULT NULL,
	`FC_ULTFECHAMOD` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
	`FC_USER` INT(11) NULL DEFAULT NULL,
	PRIMARY KEY (`FC_ID`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=6
;

`FC_ID`,`FC_CODIGO`,`FC_DESCRIPCION`,`FC_TIPOCLIENTE`,`FC_TIPODOC`,`FC_DOCUMENTO`,`FC_DIRECCION1`,`FC_DIRECCION2`,`FC_DIRECCION3`,`FC_TELEFONO1`,`FC_TELEFONO2`,`FC_MOVIL1`,`FC_MOVIL2`,`FC_PERSONACONTACTO`,`FC_EMAIL`,
`FC_EMAILALT`,`FC_FECHAINICIO`,`FC_FACEBOOK`,`FC_TWITTER`,`FC_INSTAGRAM`,`FC_DIASCREDITO`,`FC_LIMITECREDITO`,
`FC_TOLERANCIA`,`FC_TIPOPRECIO`,`FC_EXENTO`,`FC_STATUS`,`FC_FECHACREACION``FC_ULTFECHAMOD``FC_USER`

CREATE TABLE `fprovider` (
	`FP_ID` INT(10) NOT NULL AUTO_INCREMENT,
	`FP_CODE` VARCHAR(80) NULL DEFAULT NULL,
	`FP_DESCRIPTION` VARCHAR(180) NULL DEFAULT NULL,
	`FP_TYPEPROVIDER` INT(3) NULL DEFAULT '1',
	`FP_TYPEDOC` INT(3) NULL DEFAULT NULL,
	`FP_DOCUMENT` VARCHAR(50) NULL DEFAULT NULL,
	`FP_ANDRESS1` VARCHAR(200) NULL DEFAULT NULL,
	`FP_ANDRESS2` VARCHAR(200) NULL DEFAULT NULL,
	`FP_ANDRESS3` VARCHAR(200) NULL DEFAULT NULL,
	`FP_PHONO1` VARCHAR(50) NULL DEFAULT NULL,
	`FP_PHONE2` VARCHAR(50) NULL DEFAULT NULL,
	`FP_MOVIL1` VARCHAR(50) NULL DEFAULT NULL,
	`FP_MOVIL2` VARCHAR(50) NULL DEFAULT NULL,
	`FP_CONTACT` VARCHAR(50) NULL DEFAULT NULL,
	`FP_EMAIL` VARCHAR(50) NULL DEFAULT NULL,
	`FP_EMAILALT` VARCHAR(50) NULL DEFAULT NULL,
	`FP_STARDATE` DATE NULL DEFAULT NULL,
	`FP_WEBSITE` VARCHAR(50) NULL DEFAULT NULL,
	`FP_FACEBOOK` VARCHAR(50) NULL DEFAULT NULL,
	`FP_TWITTER` VARCHAR(50) NULL DEFAULT NULL,
	`FP_INSTAGRAM` VARCHAR(50) NULL DEFAULT NULL,
	`FP_CREDITDAY` INT(5) NULL DEFAULT '0',
	`FP_CREDITLIMIT` DECIMAL(10,2) NULL DEFAULT '0.00',
	`FP_PLUSDAY` INT(5) NULL DEFAULT '0',
	`FP_EXEMPT` INT(2) NULL DEFAULT '1',
	`FP_STATUS` INT(2) NULL DEFAULT '1',
	`FP_CREATEDATE` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
	`FP_LASTDATEMOD` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
	`FP_USER` INT(11) NULL DEFAULT NULL,
	PRIMARY KEY (`FP_ID`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=1
;

ALTER TABLE `fprovider`
	CHANGE COLUMN `FP_FECHACREACION` `CREATEDATE` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP AFTER `FP_STATUS`,
	CHANGE COLUMN `FP_ULTFECHAMOD` `FP_LASTDATEMOD` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP AFTER `CREATEDATE`;
